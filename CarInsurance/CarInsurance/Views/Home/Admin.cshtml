@using CarInsurance.Models
@model List<CarInsuranceNewQuote>

<div class="row no-gutters m-3">
    <div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
        <div class="card-header text-center">
            <h5 class="m-0">New Quotes</h5>
        </div>
        <div class="card-body">
            @foreach (var quote in Model)
            {
            <div class="row">
                <div class="col">
                    @quote.FirstName
                </div>
                <div class="col">
                    @quote.LastName
                </div>
                <div class="col">
                    @quote.EmailAddress
                </div>
                <div class="col">
                    @quote.DateOfBirth
                </div>
                <div class="col">
                    @quote.CarYear
                </div>
                <div class="col">
                    @quote.CarMake
                </div>
                <div class="col">
                    @quote.CarModel
                </div>
                <div class="col">
                    @quote.DUI
                </div>
                <div class="col">
                    @quote.Coverage
                </div>
            </div>
            }

        </div>

    </div>
</div>