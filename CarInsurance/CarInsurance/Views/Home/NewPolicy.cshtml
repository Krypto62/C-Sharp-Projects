@using CarInsurance.Models
@model List<CarInsuranceNewQuote>

<div class="row no-gutters m-3">
    <div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
        <div class="card-header text-center">
            <h5 class="m-0">New Quotes</h5>
        </div>
        <div class="card-body">
            @foreach (var newQuote in Model)
            {
                <div class="row">
                    <div class="col">
                        @newQuote.FirstName
                    </div>
                    <div class="col">
                        @newQuote.LastName
                    </div>
                    <div class="col">
                        @newQuote.EmailAddress
                    </div>
                    <div class="col">
                        @newQuote.DateOfBirth
                    </div>
                    <div class="col">
                        @newQuote.CarYear
                    </div>
                    <div class="col">
                        @newQuote.CarMake
                    </div>
                    <div class="col">
                        @newQuote.CarModel
                    </div>
                    <div class="col">
                        @newQuote.DUI
                    </div>
                    <div class="col">
                        @newQuote.Coverage
                    </div>
                </div>
            }

        </div>

    </div>
</div>
